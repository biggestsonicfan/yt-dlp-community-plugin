    "version": "0.0.1",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "yt_dlp",
            "args": [
                "https://www.youtube.com/post/UgkxNu6y5W5T2cUW_LcSVojcViVXfQdxZ3Y8",
                "--cookies-from-browser",
                "firefox",
                "--add-metadata",
                "--write-info-json",
                "--write-comments",
                "--ignore-errors",
                "--ignore-no-formats",
                "-P",
                "downloaded_data",
                "-o",
                "%(playlist_id,id)s/%(id)s.%(ext)s"
            ]
        }
    ]
}